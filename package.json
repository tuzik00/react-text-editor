{
  "name": "react-txt-editor",
  "description": "React content editor",
  "version": "1.1.0",
  "license": "MIT",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/tuzik00/react-txt-editor.git"
  },
  "homepage": "https://github.com/tuzik00/react-txt-editor#readme",
  "scripts": {
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -o storybook-static",
    "build:clean": "rimraf build",
    "build:ts": "ttsc",
    "build:copy": "copyfiles -u 1 ./src/assets/**/* ./build",
    "build:alias": "tsc-alias -p tsconfig.json",
    "build": "run-s build:*",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "tsc": "tsc --noEmit",
    "prepare": "husky install",
    "deploy": "run-s deploy:*",
    "deploy:clean": "rimraf build && rimraf storybook-static",
    "deploy:test": "yarn lint && yarn tsc",
    "deploy:storybook": "yarn build-storybook && gh-pages -d storybook-static",
    "deploy:app": "yarn build && yarn publish"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@emotion/css": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.10.14",
    "@mui/styles": "^5.10.14",
    "@types/react-color": "^3.0.6",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "draft-js": "^0.11.7",
    "draftjs-utils": "^0.10.2",
    "gh-pages": "^5.0.0",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.30",
    "prism-react-renderer": "^1.3.1",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-outside-click-handler": "^1.3.0",
    "rehype-stringify": "^9.0.3",
    "remark-directive": "^2.0.1",
    "remark-gfm": "^3.0.1",
    "remark-images": "^3.1.0",
    "remark-parse": "^10.0.1",
    "remark-supersub": "^1.0.0",
    "remarkable": "^2.0.1",
    "tsc-alias": "^1.7.1",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.0",
    "use-debounce": "^6.0.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.2",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addons": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/node-logger": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.10",
    "@storybook/theming": "^6.5.10",
    "@svgr/webpack": "^6.3.1",
    "@types/draft-js": "^0.11.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.17",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-outside-click-handler": "^1.3.1",
    "@types/remarkable": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "audit-ci": "^6.2.0",
    "babel-loader": "^8.2.5",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.7.1",
    "esbuild-loader": "^2.20.0",
    "eslint": "^7.3.2",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.0",
    "install-peers-cli": "^2.2.0",
    "lint-staged": "^11.2.6",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "^2.1.9",
    "react-scripts": "^5.0.1",
    "react-svg-loader": "^3.0.3",
    "rimraf": "^3.0.2",
